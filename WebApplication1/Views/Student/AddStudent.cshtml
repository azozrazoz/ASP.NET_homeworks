@model List<WebApplication1.Models.Student>
@{
    ViewBag.Title = "AddStudent";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Add Student</h2>

@using (Html.BeginForm())
            {
                for(int i = 0; i < Model.Count; i++)
                {
                    <h3>Student N@(i + 1)</h3>

                    @: Name: @Html.EditorFor(m => m[i].Name);
                }
    <input type="submit" />
            }